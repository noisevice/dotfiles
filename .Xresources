#################
! URXVT Settings
#################

!URxvt.font:xft:Schumacher Clean:pixelsize=10
!URxvt.letterSpace: 0

URxvt.font:xft:Fira Mono Medium:size=8
URxvt.letterSpace: -2

URxvt.internalBorder: 16
URxvt.scrollBar: false
URxvt.boldFont:
URxvt.perl-ext:
URxvt.perl-ext-common:

###############
! Xft Settings
###############

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull
Xft.autohint:   true
Xft.lcdfilter: lcddefault

####################
! Xresources Colors
####################

*foreground: #DDCCBB

! Brown BG
!*background: #30251D

! Grey BG
*background: #151515


!black
